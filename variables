#include <iostream>

int main() {
  
  double height, weight, bmi;
  
  // Ask user for their height
  
  std::cout << "Type in your height (m): ";
  std::cin >> height;
  
  // Now ask the user for their weight and calculate BMI
  std::cout<< "Type in your weight in kgs: ";
  std::cin >> weight;
  
  bmi = weight/ pow(height,2);
  
  std::cout << "BMI :  " << bmi << "\n";
    
  return 0;

}
